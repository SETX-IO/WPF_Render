<Window x:Class="Driextx_WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Driextx_WPF"
        xmlns:interop="clr-namespace:System.Windows.Interop;assembly=PresentationCore"
        xmlns:v="clr-namespace:Vortice;assembly=Vortice.Wpf"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="MainWindow" Height="720" Width="1280">
    
    <Grid>
        <Image x:Name="imgelt" Loaded="MainWindow_OnLoaded">
            <Image.Source>
                <interop:D3DImage x:Name="d3dimg"/>
            </Image.Source>
        </Image>
    </Grid>
</Window>
